<?xml version = "1.0" encoding = "UTF-8" ?>
<Application xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:Wsinvmovil.Servicios"
             x:Class="Wsinvmovil.App">
    <Application.Resources>
        <ResourceDictionary>
            <local:SelectionColorConverter x:Key="SelectionColorConverter"/>
            <Color x:Key="PrimaryColor">LightCyan</Color>
            <Color x:Key="SecondaryColor">#2196F3</Color>
            <Color x:Key="ThirdColor">#EDEBF6</Color>
            <Color x:Key="TextColor">#222222</Color>
            <Color x:Key="Salir">green</Color>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/Styles/Colors.xaml" />
                <ResourceDictionary Source="Resources/Styles/Styles.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
